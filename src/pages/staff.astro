---
import DefaultLayout from '../layouts/DefaultLayout.astro'
const { staff, name,title, image, description } = Astro.props;
---

<DefaultLayout title={title} >
  <section class="section">
    <div class="container">
      <h1 class="text-center font-normal" set:html={(title)} />
      <div class="section row -mt-6">
        {
          staff?.map((staff) => (
            <div class="col-12 mt-6 md:col-6">
              <div class="p-12  shadow">
                <div class="faq-head relative">
                  <h4 set:html={(staff.name)} />
                </div>
                <img src={staff.image} />
                <p class="faq-body mt-4" set:html={(staff.title)} />
              </div>
            </div>
          ))
        }
      </div>
    </div>
  </section>
</Base>
